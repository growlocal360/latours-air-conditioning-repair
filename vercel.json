{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    { "source": "/about-us", "destination": "/about/", "permanent": true },
    { "source": "/about-us/", "destination": "/about/", "permanent": true },
    { "source": "/air-conditioning", "destination": "/ac-contractor/", "permanent": true },
    { "source": "/air-conditioning/", "destination": "/ac-contractor/", "permanent": true },
    { "source": "/services", "destination": "/hvac-contractor/", "permanent": true },
    { "source": "/services/", "destination": "/hvac-contractor/", "permanent": true },
    { "source": "/maintenance", "destination": "/hvac-contractor/hvac-maintenance/", "permanent": true },
    { "source": "/maintenance/", "destination": "/hvac-contractor/hvac-maintenance/", "permanent": true },
    { "source": "/ac-repair-sulphur-la", "destination": "/service-areas/sulphur-la/", "permanent": true },
    { "source": "/ac-repair-sulphur-la/", "destination": "/service-areas/sulphur-la/", "permanent": true },
    { "source": "/ac-repair-iowa-la", "destination": "/service-areas/iowa-la/", "permanent": true },
    { "source": "/ac-repair-iowa-la/", "destination": "/service-areas/iowa-la/", "permanent": true },
    { "source": "/air-conditioning-repair-westlake-la", "destination": "/service-areas/westlake-la/", "permanent": true },
    { "source": "/air-conditioning-repair-westlake-la/", "destination": "/service-areas/westlake-la/", "permanent": true },
    { "source": "/heat-pump", "destination": "/heating-contractor/heat-pump-repair/", "permanent": true },
    { "source": "/heat-pump/", "destination": "/heating-contractor/heat-pump-repair/", "permanent": true }
  ],
  "headers": [
    {
      "source": "/css/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/js/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
